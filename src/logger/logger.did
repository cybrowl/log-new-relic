type Tags = 
 vec record {
       text;
       text;
     };
type Message = text;
type LogEvent = 
 record {
   message: text;
   tags: Tags;
   time: int;
 };
service : {
  authorize: () -> (bool);
  clear_logs: () -> (text);
  get_logs: () -> (vec LogEvent) query;
  log_event: (Tags, Message) -> ();
  version: () -> (nat) query;
  whoami: () -> (text);
}
